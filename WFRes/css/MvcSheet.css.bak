div.SheetRadioButtonList > div[gropname] > input[type="radio"], div.SheetCheckBoxList > div[gropname] > input[type="checkbox"] {
    display: none;
}

    div.SheetRadioButtonList > div[gropname] > input[type="radio"] + label, div.SheetCheckBoxList > div[gropname] > input[type="checkbox"] + label {
        display: inline-block;
        width: 60%;
        float: right;
        position: relative;
        text-align: left;
        padding: 10px 0 0 0;
    }

        div.SheetCheckBoxList > div[gropname] > input[type="checkbox"] + label:before {
            background-color: #fafafa;
            border: 1px solid #cacece;
            -moz-border-radius: 3px;
            -webkit-border-radius: 3px;
            border-radius: 3px;
            display: block;
            position: absolute;
            top: 8px;
            left: -25px;
            width: 1.3em;
            height: 1.3em;
            content: " ";
        }

    div.SheetCheckBoxList > div[gropname] > input[type="checkbox"]:checked + label:before {
        content: '\00a0\2714';
        padding: 0;
        display: inline-block;
    }

    div.SheetRadioButtonList > div[gropname] > input[type="radio"]:not(.toggle) + label:before {
        background-color: #fafafa;
        border: 1px solid #cacece;
        -moz-border-radius: 50px;
        -webkit-border-radius: 50px;
        border-radius: 50px;
        display: block;
        position: absolute;
        width: 1.3em;
        height: 1.3em;
        content: '';
        margin-right: 5px;
        top: 8px;
        margin-left: -25px;
    }

    div.SheetRadioButtonList > div[gropname] > input[type="radio"]:checked + label:before {
        background-color: #000;
    }

