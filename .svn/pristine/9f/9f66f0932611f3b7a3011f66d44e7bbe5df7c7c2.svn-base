/*动态点赞开始*/
.praise{
    margin: 50px auto;
    cursor: pointer;
    font-size: 12px;
    text-align: center;
    position: relative;
}
#praise{
    display:block;
    width:80px;
    height:80px;
    margin:0 auto;
}
#praise-txt{
    height:25px;
    line-height:45px;
    font-size: 22px;
}

.praise {
    text-align: center
}

.praise img{
    width:80px;
    height:80px;
    display:block;
    margin: 0 auto;
}
.praise img.animation{
    animation: myfirst 0.5s;
    -moz-animation: myfirst 0.5s;	/* Firefox */
    -webkit-animation: myfirst 0.5s;	/* Safari 和 Chrome */
    -o-animation: myfirst 0.5s;	/* Opera */
}
#add-num{
    display:none;
}
#add-num .add-animation{
    color: #000;
    position:absolute;
    top: -16px;
    left: 50%;
    font-size: 22px;
    opacity: 0;
    filter: Alpha(opacity=0);
    -moz-opacity:0;
    animation: mypraise 0.5s ;
    -moz-animation: mypraise 0.5s ;	/* Firefox */
    -webkit-animation: mypraise 0.5s ;	/* Safari 和 Chrome */
    -o-animation: mypraise 0.5s ;	/* Opera */
    font-style:normal;
}
.praise .hover  , #praise-txt.hover{
    color: #419bdd;
    font-size: 22px;
}

#add-num .add-animation.hover {
    color: #EB4F38;
}

