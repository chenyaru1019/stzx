					dtl._AddRow();
				}
				$.each(result, function(index, term) {
					//遍历，给子表赋值
					 $.MvcSheetUI.SetControlValue(&quot;SMADetails.Type&quot;,
							term.Type, index + 1);
					 $.MvcSheetUI.SetControlValue(&quot;SMADetails.MaterialName&quot;,
